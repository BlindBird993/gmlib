############
# Pre-config
# Config vars
set( LIBRARY @VAR_LIBRARY@ )
set( INC_RELPATH @VAR_INC_RELPATH@ )

# Set the path of this file
get_filename_component( LIB_CMAKE_DIR ${CMAKE_CURRENT_LIST_FILE} PATH )

#######################
# Add include directory
include_directories( ${LIB_CMAKE_DIR}/${INC_RELPATH} )

######################
# Set libraries
string( TOUPPER ${LIBRARY} LIBRARY_UPPER )
set( ${LIBRARY_UPPER}_LIBRARIES "-L${LIB_CMAKE_DIR}/@VAR_LIB_RELPATH@ @VAR_LIBS@" )

